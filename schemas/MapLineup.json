{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"additionalProperties": true,
	"description": "A Map Lineup",

	"properties": {
		"scoreaim": {
			"type": "integer",
			"description": "best of/first to maps",
			"default": 3,
			"minimum": 1,
			"maximum": 9
		},
		"scoringType": {
			"type": "string",
			"description": "best of or first to",
			"enum": ["ft", "bo"]
		},
		"maps": {
			"tsType": "MapSelection[]",
			"description": "the selection of maps",
			"default": []
		}
	},
	"required": ["scoreaim", "scoringType", "maps"]
}
